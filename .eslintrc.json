{
  "extends": "@rocketseat/eslint-config/react", // Se for node, use apenas esse
  "globals": {
    "React": true,
    "google": true,
    "mount": true,
    "mountWithRouter": true,
    "shallow": true,
    "shallowWithRouter": true,
    "context": true,
    "expect": true,
    "jsdom": true,
    "JSX": true
  },
  "rules": {
    "@typescript-eslint/no-empty-interface": "off",
    "camelcase":"off",
    "no-useless-catch": "off",
    "react-hooks/exhaustive-deps": "off"
  }
}